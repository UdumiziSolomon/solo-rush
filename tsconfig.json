{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "noEmit": true,
    "target": "esnext",
    "baseUrl": ".",
    "paths": {
      "assets/*": ["assets/*"],
      "components/*": ["src/components/*"],
      "hooks/*": ["src/hooks/*"],
      "modules/*": ["src/modules/*"],
      "navigations/*": ["src/navigations/*"],
      "pages/*": ["src/pages/*"],
      "svgs/*": ["src/svgs/*"],
      "types/*": ["src/types/*"],
      "utils/*": ["src/utils/*"]
    }
  },
}
